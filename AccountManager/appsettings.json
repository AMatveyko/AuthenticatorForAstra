{
  "authenticatorSettings": {
    "secret": "123123123",
    "debugFilePath": "/var/log/pamAuthDebug.log",
    "defaultUserGroup": "readers"
  },
  "irbisSettings": {
    "host": "10.0.0.157",
    "port": "6666",
    "login": "1",
    "password": "1"
  },
  ,
  "NLog":{
    "internalLogLevel":"Info",
    "internalLogFile":"/var/log/internalNLog.log",
    "extensions": [
      { "assembly": "NLog.Extensions.Logging" },
      { "assembly": "NLog.Web.AspNetCore" }
    ],
    "targets":{
      "allExceptions":{
        "type": "File",
        "fileName": "/var/log/pamAuthenticator-errors-${shortdate}.log",
        "layout": "${longdate}|${uppercase:${level}}|${logger}|${message} ${exception:format=tostring}",
        "maxArchiveFiles": "10"
      },
      "debugTarget": {
        "type": "File",
        "fileName": "/var/log/pamAuthenticator-debug-${shortdate}.log",
        "layout": "${longdate} ${message}",
        "maxArchiveFiles": "10"
      }
    },
    "rules": [
      {
        "logger":"allExceptions",
        "minLevel":"Trace",
        "writeTo":"allExceptions"
      },
      {
        "logger": "debug",
        "minLevel": "Trace",
        "writeTo": "debugTarget"
      }
    ]
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://0.0.0.0:5005",
        "Protocols": "Http2"
      }
    }
  }
}
